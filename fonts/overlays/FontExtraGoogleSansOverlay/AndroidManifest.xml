<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.theme.font.google_sans">

    <overlay
        android:targetPackage="android"
        android:category="android.theme.customization.font"
        android:priority="1" />

    <application
        android:label="@string/font_google_sans_overlay"
        android:hasCode="false">
        <meta-data
            android:name="android.theme.customization.REQUIRED_SYSTEM_FONTS"
            android:value="google-sans,google-sans-thin,google-sans-light,google-sans-medium,google-sans-black,google-sans-text,google-sans-text-thin,google-sans-text-light,google-sans-text-medium,google-sans-text-black" />
    </application>

</manifest>
